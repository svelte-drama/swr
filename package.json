{
  "name": "@svelte-drama/swr",
  "version": "1.3.1",
  "scripts": {
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "dev": "vite dev --open",
    "format": "prettier . --write .",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepare": "husky install",
    "prepublishOnly": "npm run package"
  },
  "devDependencies": {
    "@svelte-drama/suspense": "^0.4.6",
    "@sveltejs/adapter-auto": "^3.1.0",
    "@sveltejs/kit": "^2.3.3",
    "@sveltejs/package": "^2.2.6",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "cross-spawn": "^7.0.3",
    "husky": "^8.0.3",
    "prettier": "^3.2.2",
    "prettier-plugin-svelte": "^3.1.2",
    "publint": "^0.2.7",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.3",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.11"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "homepage": "https://github.com/svelte-drama/swr#readme",
  "license": "0BSD",
  "packageManager": "pnpm@8.3.1",
  "peerDependencies": {
    "svelte": "^3.54.0 || ^4.0.0"
  },
  "repository": "github:svelte-drama/swr",
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module"
}
