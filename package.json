{
  "name": "@svelte-drama/swr",
  "version": "3.0.0",
  "scripts": {
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "dev": "vite dev --open",
    "format": "prettier . --write .",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepare": "husky",
    "prepublishOnly": "npm run package"
  },
  "devDependencies": {
    "@svelte-drama/suspense": "^1.0.1",
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/kit": "^2.20.7",
    "@sveltejs/package": "^2.3.11",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "cross-spawn": "^7.0.6",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "publint": "^0.3.12",
    "svelte": "^5.27.0",
    "svelte-check": "^4.1.6",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "vite": "^6.2.6"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "homepage": "https://github.com/svelte-drama/swr#readme",
  "license": "0BSD",
  "packageManager": "pnpm@10.2.0",
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "repository": "github:svelte-drama/swr",
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module"
}
