{
  "name": "@svelte-drama/swr",
  "version": "2.0.0",
  "scripts": {
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "dev": "vite dev --open",
    "format": "prettier . --write .",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepare": "husky install",
    "prepublishOnly": "npm run package"
  },
  "devDependencies": {
    "@svelte-drama/suspense": "0.6.0-next.0",
    "@sveltejs/adapter-auto": "^3.2.5",
    "@sveltejs/kit": "^2.6.1",
    "@sveltejs/package": "^2.3.5",
    "@sveltejs/vite-plugin-svelte": "4.0.0-next.7",
    "cross-spawn": "^7.0.3",
    "husky": "^9.1.6",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.7",
    "publint": "^0.2.11",
    "svelte": "5.0.0-next.260",
    "svelte-check": "^4.0.4",
    "tslib": "^2.7.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.8"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "homepage": "https://github.com/svelte-drama/swr#readme",
  "license": "0BSD",
  "packageManager": "pnpm@8.3.1",
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "repository": "github:svelte-drama/swr",
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module"
}
