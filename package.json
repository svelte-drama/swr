{
  "name": "@svelte-drama/swr",
  "version": "1.0.0",
  "scripts": {
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "dev": "vite dev",
    "format": "prettier --plugin-search-dir . --write .",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepare": "husky install",
    "prepublishOnly": "npm run package"
  },
  "devDependencies": {
    "@svelte-drama/suspense": "^0.4.1",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.20.5",
    "@sveltejs/package": "^2.1.0",
    "cross-spawn": "^7.0.3",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "publint": "^0.1.12",
    "svelte": "^4.0.0",
    "svelte-check": "^3.4.4",
    "tslib": "^2.6.0",
    "typescript": "^5.1.3",
    "vite": "^4.3.9"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "homepage": "https://github.com/svelte-drama/swr#readme",
  "license": "0BSD",
  "packageManager": "pnpm@8.3.1",
  "peerDependencies": {
    "svelte": "^3.54.0 || ^4.0.0"
  },
  "repository": "github:svelte-drama/swr",
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module"
}
